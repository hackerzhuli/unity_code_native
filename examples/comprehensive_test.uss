    border-radius: 5px 10px 15px 20px;
}

.complex-border-radius-2 {
    border-radius: 8px 12px 16px;
}

.complex-border-radius-3 {
    border-radius: 10px 20px;
}

.complex-border-radius-4 {
    border-radius: 15px;
}

/* Various color formats - text color */
.complex-text-color-1 {
    color: red;
}

.complex-text-color-2 {
    color: #ff0000;
}

.complex-text-color-3 {
    color: #f00;
}

.complex-text-color-4 {
    color: rgb(255, 0, 0);
}

.complex-text-color-5 {
    color: rgba(255, 0, 0, 0.8);
}

/* Various color formats - background color */
.complex-bg-color-1 {
    background-color: blue;
}

.complex-bg-color-2 {
    background-color: #0000ff;
}

.complex-bg-color-3 {
    background-color: #00f;
}

.complex-bg-color-4 {
    background-color: rgb(0, 0, 255);
}

.complex-bg-color-5 {
    background-color: rgba(0, 0, 255, 0.6);
}

/* Various border color combinations */
.complex-border-color-1 {
    border-color: green yellow purple orange;
}

.complex-border-color-2 {
    border-color: #00ff00 #ffff00 #800080;
}

.complex-border-color-3 {
    border-color: rgb(0, 255, 0) rgba(255, 255, 0, 0.7);
}